#include <stdio.h>

// Function declaration
int findMaximum(int a, int b);

int main() {
    int num1, num2, max;

    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    // Function call
    max = findMaximum(num1, num2);

    printf("The maximum number is: %d\n", max);

    return 0;
}

// Function definition
int findMaximum(int a, int b) {
    if (a > b)
        return a;
    else
        return b;
}