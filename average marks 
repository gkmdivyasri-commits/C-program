#include <stdio.h>

int main() {
    int marks[5], i, sum = 0;
    float average;

    printf("Enter marks of 5 subjects:\n");

    for (i = 0; i < 5; i++) {
        scanf("%d", &marks[i]);
        sum += marks[i];
    }

    average = sum / 5.0;

    printf("Average Marks = %.2f", average);

    return 0;
}