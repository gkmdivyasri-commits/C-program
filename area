#include <stdio.h>

// Function to calculate area
int area(int length, int width) {
    return length * width;
}

int main() {
    int length, width;

    printf("Enter length: ");
    scanf("%d", &length);

    printf("Enter width: ");
    scanf("%d", &width);

    printf("Area of rectangle = %d\n", area(length, width));

    return 0;
}