#include <stdio.h>

int main()
{
    int n, i, j, temp;
    int marks[50];

    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter marks of students:\n");
    for(i = 0; i < n; i++)
    {
        scanf("%d", &marks[i]);
    }

    // Sorting marks (Ascending order)
    for(i = 0; i < n - 1; i++)
    {
        for(j = i + 1; j < n; j++)
        {
            if(marks[i] > marks[j])
            {
                temp = marks[i];
                marks[i] = marks[j];
                marks[j] = temp;
            }
        }
    }

    printf("Sorted marks (Ascending Order):\n");
    for(i = 0; i < n; i++)
    {
        printf("%d ", marks[i]);
    }

    return 0;
}