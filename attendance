#include <stdio.h>

int main() {
    int n, i;

    // Ask for the number of students
    printf("Enter the number of students: ");
    scanf("%d", &n);

    // Print attendance numbers
    printf("Attendance numbers of students:\n");
    for(i = 1; i <= n; i++) {
        printf("Student %d\n", i);
    }

    return 0;
}