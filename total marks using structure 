#include <stdio.h>

struct Student {
    int marks1;
    int marks2;
    int marks3;
    int total;
};

int main() {
    struct Student s;

    printf("Enter marks of subject 1: ");
    scanf("%d", &s.marks1);

    printf("Enter marks of subject 2: ");
    scanf("%d", &s.marks2);

    printf("Enter marks of subject 3: ");
    scanf("%d", &s.marks3);

    s.total = s.marks1 + s.marks2 + s.marks3;

    printf("Total Marks = %d\n", s.total);

    return 0;
}